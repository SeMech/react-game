{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["last 1 version"],
        "safari": "9",
        "ie": "11",
        "ios": "9",
        "android": "4"
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/plugin-transform-regenerator"
  ]
}
